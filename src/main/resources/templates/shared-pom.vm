	<dependencyManagement>
		<dependencies>
			<dependency>
				<groupId>pegasus.framework.mdk</groupId>
				<artifactId>extdep</artifactId>
				<version>${extdep.version}</version>
				<type>pom</type>
				<scope>import</scope>
			</dependency>
                        <dependency>
				<groupId>pegasus.framework.pdk.pegasus</groupId>
				<artifactId>pegasus-core</artifactId>
				<version>${pegasus-core.version}</version>
			</dependency>
                        #if($tfw)
