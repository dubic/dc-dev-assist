				<groupId>pegasus.module.storeandforward</groupId>
				<artifactId>storeandforward-shared</artifactId>
				<version>${storeandforward-shared.version}</version>
			</dependency>
			<dependency>
				<groupId>pegasus.module.storeandforward</groupId>
				<artifactId>storeandforward-client-common</artifactId>
				<version>${storeandforward-client-common.version}</version>
			</dependency>
			<dependency>
				<groupId>pegasus.module.storeandforward</groupId>
				<artifactId>storeandforward-spi</artifactId>
				<version>${storeandforward-spi.version}</version>
			</dependency>
			<dependency>
				<groupId>pegasus.module.customer</groupId>
				<artifactId>customer-shared</artifactId>
				<version>${customer-shared.version}</version>
			</dependency>
                        #end
		</dependencies>
	</dependencyManagement>
	
	<dependencies>
		<dependency>
			<groupId>pegasus.framework.pdk.pegasus</groupId>
