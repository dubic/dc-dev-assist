                <storeandforward-shared.version>1.6.0</storeandforward-shared.version>
		<storeandforward-client-common.version>3.2.0</storeandforward-client-common.version>
		<storeandforward-spi.version>3.2.0</storeandforward-spi.version>
		<customer-shared.version>3.3.0EQU</customer-shared.version>
